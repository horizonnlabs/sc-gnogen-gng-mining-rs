{
  "name": "stake battle 1",
  "steps": [
    {
      "step": "externalSteps",
      "path": "02_deposit_gng.scen.json"
    },
    {
      "step": "setState",
      "currentBlockInfo": {
        "blockTimestamp": "1000"
      }
    },
    {
      "step": "scCall",
      "txId": "hacker1 try to stake fake nft",
      "tx": {
        "from": "address:hacker1",
        "to": "sc:gng-minting",
        "function": "stake",
        "arguments": [],
        "esdtValue": [
          {
            "tokenIdentifier": "str:FAKEGNOGONS-abcdef",
            "nonce": "1",
            "value": "1"
          }
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "status": "4",
        "message": "str:Wrong token",
        "out": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scCall",
      "txId": "user1 stake emidas-1",
      "tx": {
        "from": "address:user1",
        "to": "sc:gng-minting",
        "function": "stake",
        "arguments": [],
        "esdtValue": [
          {
            "tokenIdentifier": "str:EMIDAS-abcdef",
            "nonce": "1",
            "value": "1"
          },
          {
            "tokenIdentifier": "str:GNOGONS-abcdef",
            "nonce": "1",
            "value": "1"
          }
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "status": "0",
        "message": "",
        "out": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scQuery",
      "txId": "get staked for address user1",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getStakedForAddress",
        "arguments": ["address:user1", "str:EMIDAS-abcdef"]
      },
      "expect": {
        "out": ["1"]
      }
    },
    {
      "step": "scQuery",
      "txId": "get first stack",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getFirstStack",
        "arguments": ["1"]
      },
      "expect": {
        "out": [
          [
            {
              "0-tokenIdentifier": "nested:str:EMIDAS-abcdef",
              "1-nonce": "u64:1"
            }
          ]
        ]
      }
    },
    {
      "step": "scQuery",
      "txId": "get second stack",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getSecondStack",
        "arguments": ["1"]
      },
      "expect": {
        "out": [
          [
            {
              "0-tokenIdentifier": "nested:str:GNOGONS-abcdef",
              "1-nonce": "u64:1"
            }
          ]
        ]
      }
    }
  ]
}
