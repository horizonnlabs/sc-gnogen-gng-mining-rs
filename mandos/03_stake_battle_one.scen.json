{
  "name": "stake battle 1",
  "steps": [
    {
      "step": "externalSteps",
      "path": "02_deposit_gng.scen.json"
    },
    {
      "step": "setState",
      "currentBlockInfo": {
        "blockTimestamp": "1000"
      }
    },
    {
      "step": "scCall",
      "txId": "hacker1 try to stake fake nft",
      "tx": {
        "from": "address:hacker1",
        "to": "sc:gng-minting",
        "function": "stake",
        "arguments": [],
        "esdtValue": [
          {
            "tokenIdentifier": "str:FAKEGNOGONS-abcdef",
            "nonce": "1",
            "value": "1"
          }
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "status": "4",
        "message": "str:Wrong token",
        "out": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scCall",
      "txId": "user1 stake emidas-1 gnogons-1",
      "tx": {
        "from": "address:user1",
        "to": "sc:gng-minting",
        "function": "stake",
        "arguments": [],
        "esdtValue": [
          {
            "tokenIdentifier": "str:EMIDAS-abcdef",
            "nonce": "1",
            "value": "1"
          },
          {
            "tokenIdentifier": "str:GNOGONS-abcdef",
            "nonce": "1",
            "value": "1"
          }
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "status": "0",
        "message": "",
        "out": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scQuery",
      "txId": "get staked for address user1",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getStakedForAddress",
        "arguments": ["address:user1", "str:EMIDAS-abcdef"]
      },
      "expect": {
        "out": ["1"]
      }
    },
    {
      "step": "scQuery",
      "txId": "get all staked for address user1",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getAllStakedForAddress",
        "arguments": ["address:user1"]
      },
      "expect": {
        "out": ["str:EMIDAS-abcdef", "1", "str:GNOGONS-abcdef", "1"]
      }
    },
    {
      "step": "scQuery",
      "txId": "get total nfts engaged",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getTotalNftEngaged",
        "arguments": []
      },
      "expect": {
        "out": ["2"]
      }
    },
    {
      "step": "scQuery",
      "txId": "get stats for user1",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getStatsForAddress",
        "arguments": ["address:user1"]
      },
      "expect": {
        "out": [
          {
            "0-win": "u64:0",
            "1-loss": "u64:0",
            "2-gng_claimed": "biguint:0"
          }
        ]
      }
    },
    {
      "step": "scQuery",
      "txId": "get stats for nft emidas 1",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getStatsForNft",
        "arguments": ["str:EMIDAS-abcdef", "1"]
      },
      "expect": {
        "out": [
          "str:EMIDAS-abcdef", "1",
          {
            "0-win": "u64:0",
            "1-loss": "u64:0",
            "2-owner": "address:user1",
            "3-staked-id": "u64:1"
          }
        ]
      }
    },
    {
      "step": "scQuery",
      "txId": "get battle stack",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getBattleStack",
        "arguments": ["1"]
      },
      "expect": {
        "out": [
          {
            "0-tokenIdentifier": "nested:str:EMIDAS-abcdef",
            "1-nonce": "u64:1",
            "2-id": "u64:1"
          },
          {
            "0-tokenIdentifier": "nested:str:GNOGONS-abcdef",
            "1-nonce": "u64:1",
            "2-id": "u64:2"
          }
        ]
      }
    },
    {
      "step": "scCall",
      "txId": "user3 stake gnogons-3 gnogons-4",
      "tx": {
        "from": "address:user3",
        "to": "sc:gng-minting",
        "function": "stake",
        "arguments": [],
        "esdtValue": [
          {
            "tokenIdentifier": "str:GNOGONS-abcdef",
            "nonce": "3",
            "value": "1"
          },
          {
            "tokenIdentifier": "str:GNOGONS-abcdef",
            "nonce": "4",
            "value": "1"
          }
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "status": "0",
        "message": "",
        "out": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scCall",
      "txId": "user4 stake emidas-3",
      "tx": {
        "from": "address:user4",
        "to": "sc:gng-minting",
        "function": "stake",
        "arguments": [],
        "esdtValue": [
          {
            "tokenIdentifier": "str:EMIDAS-abcdef",
            "nonce": "3",
            "value": "1"
          }
        ],
        "gasLimit": "20,000,000",
        "gasPrice": "0"
      },
      "expect": {
        "status": "0",
        "message": "",
        "out": [],
        "gas": "*",
        "refund": "*"
      }
    },
    {
      "step": "scQuery",
      "txId": "get battle stack",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getBattleStack",
        "arguments": ["1"]
      },
      "expect": {
        "out": [
          {
            "0-tokenIdentifier": "nested:str:EMIDAS-abcdef",
            "1-nonce": "u64:1",
            "2-id": "u64:1"
          },
          {
            "0-tokenIdentifier": "nested:str:GNOGONS-abcdef",
            "1-nonce": "u64:1",
            "2-id": "u64:2"
          },
          {
            "0-tokenIdentifier": "nested:str:GNOGONS-abcdef",
            "1-nonce": "u64:3",
            "2-id": "u64:3"
          },
          {
            "0-tokenIdentifier": "nested:str:GNOGONS-abcdef",
            "1-nonce": "u64:4",
            "2-id": "u64:4"
          },
          {
            "0-tokenIdentifier": "nested:str:EMIDAS-abcdef",
            "1-nonce": "u64:3",
            "2-id": "u64:5"
          }
        ]
      }
    },
    {
      "step": "scQuery",
      "txId": "get total nfts engaged",
      "tx": {
        "to": "sc:gng-minting",
        "function": "getTotalNftEngaged",
        "arguments": []
      },
      "expect": {
        "out": ["5"]
      }
    }
  ]
}
